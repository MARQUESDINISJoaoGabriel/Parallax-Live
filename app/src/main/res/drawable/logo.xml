<svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
    <!-- Background Circle -->
    <circle cx="100" cy="100" r="90" fill="#4285F4" />

    <!-- Camera Lens -->
    <circle cx="100" cy="100" r="60" fill="#FFFFFF" />
    <circle cx="100" cy="100" r="50" fill="#34A853" />
    <circle cx="100" cy="100" r="35" fill="#000000" />
    <circle cx="100" cy="100" r="25" fill="#FBBC05" />

    <!-- Camera Flash -->
    <circle cx="140" cy="70" r="15" fill="#FFFFFF" />
    <circle cx="140" cy="70" r="10" fill="#EA4335" />

    <!-- Live Text -->
    <rect x="70" y="150" width="60" height="20" rx="10" fill="#EA4335" />
    <text x="100" y="165" font-family="Arial" font-size="14" font-weight="bold" text-anchor="middle" fill="white">LIVE</text>

    <!-- Parallax Rings -->
    <circle cx="100" cy="100" r="80" fill="none" stroke="#FFFFFF" stroke-width="2" stroke-dasharray="5,5" />
    <circle cx="100" cy="100" r="70" fill="none" stroke="#FFFFFF" stroke-width="2" stroke-dasharray="3,3" />
</svg>